@model List<ecommerce.Models.Category>

@{
    ViewBag.Title = "get all Page";
}

<h1>Hola!</h1>
@foreach (var c in Model)
{
    <div class="row">
        @Html.Label(c.Name, new { @class = "control-label col-md-6" })

        @Html.Display(c.Id.ToString())
        @Html.Display(c.Name)
        @Html.Display(c.Doomy)
    </div>
}
